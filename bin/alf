#!/usr/bin/env ruby
module AlfLauncher

  def self.load
    require "alf-shell"
  end
  
  def self.start(argv)
    load
    if ENV["ALF_OPTS"]
      argv = Quickl.parse_commandline_args(ENV["ALF_OPTS"]) + argv
    end
    Alf::Shell::Main.run(argv, __FILE__)
  end

end # module AlfLaucher
AlfLauncher.start(ARGV)
